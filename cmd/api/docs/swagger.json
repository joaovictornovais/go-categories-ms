            ],
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "controllers.deleteCategoryInput": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "controllers.listCategoriesInput": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "controllers.updateCategoryInput": {
            "type": "object",
            "required": [
                "currentName",
                "newName"
            ],
            "properties": {
                "currentName": {
                    "type": "string"
                },
                "newName": {
                    "type": "string"
                }
            }
        },
        "entities.Category": {
            "type": "object",
            "properties": {
                "createdAt": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        }
